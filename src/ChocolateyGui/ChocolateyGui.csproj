    <None Remove="**/*.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="**/*.png;**/*.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Include="ChocolateyGui.exe.manifest" />
  </ItemGroup>

